{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^build", "^test"],
      "inputs": ["$TURBO_DEFAULT$"]
    },
    "docs": {
      "outputs": ["docs/**"]
    }
  }
}
